{
  "name": "ds-portfolio-eleventy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run css:watch & npx @11ty/eleventy --serve",
    "build": "npm run css && npx @11ty/eleventy",
    "css": "npx tailwindcss -i src/styles/tailwind.css -o src/assets/main.css --minify",
    "css:watch": "npx tailwindcss -i src/styles/tailwind.css -o src/assets/main.css --watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "markdown-it": "^14.1.0"
  }
}
